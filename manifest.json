{
    "name": "Send To TeamCity",
    "version": "1.0",
    "description": "Create Teamcity Build of current page",
    "manifest_version": 2,
    "options_page": "build/index.html",
    "permissions": [
        "storage",
        "notifications",
        "activeTab",
        "*://*/*"
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "browser_action": {},
    "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'"

}
